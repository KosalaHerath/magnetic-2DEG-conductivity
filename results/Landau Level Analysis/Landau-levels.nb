 RowBox[{
  RowBox[{
   SubscriptBox["\[Phi]", "n_"], "[", "x_", "]"}], " ", ":=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"n", ">", "0"}], ",", " ", 
     RowBox[{
      FractionBox["1", 
       SqrtBox[
        RowBox[{"n", "!"}]]], 
      SuperscriptBox[
       RowBox[{"(", 
        SqrtBox[
         FractionBox[
          RowBox[{"m", " ", "\[Omega]"}], "2"]], ")"}], "n"], 
      RowBox[{"Nest", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"x", " ", "#"}], " ", "-", " ", 
          RowBox[{
           FractionBox["1", 
            RowBox[{"m", " ", "\[Omega]"}]], " ", 
           RowBox[{
            SubscriptBox["\[PartialD]", "x"], "#"}]}]}], "&"}], ",", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           FractionBox[
            RowBox[{"m", " ", "\[Omega]"}], "\[Pi]"], ")"}], 
          RowBox[{"1", "/", "4"}]], 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"-", 
           FractionBox[
            RowBox[{"m", " ", "\[Omega]", " ", 
             SuperscriptBox["x", "2"]}], "2"]}]]}], " ", ",", "n"}], "]"}]}], 
     ",", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"m", " ", "\[Omega]"}], "\[Pi]"], ")"}], 
       RowBox[{"1", "/", "4"}]], 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", 
        FractionBox[
         RowBox[{"m", " ", "\[Omega]", " ", 
          SuperscriptBox["x", "2"]}], "2"]}]]}]}], "]"}], "]"}]}]], "Input",
